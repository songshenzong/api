{
  "name": "songshenzong/api",
  "description": "A RESTful API package for the Laravel",
  "keywords": [
    "restful",
    "api",
    "laravel",
    "response",
    "web"
  ],
  "license": "MIT",
  "homepage": "http://songshenzong.com",
  "support": {
    "issues": "https://github.com/songshenzong/api/issues",
    "source": "https://github.com/songshenzong/api"
  },
  "authors": [
    {
      "name": "Songshenzong",
      "email": "i@songshenzong.com"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "autoload": {
    "psr-4": {
      "Songshenzong\\Api\\": "src/"
    },
    "files": [
      "src/Helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Songshenzong\\Api\\ServiceProvider"
      ],
      "aliases": {
        "SongshenzongApi": "Songshenzong\\Api\\Facade"
      }
    }
  }
}
